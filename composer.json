{
  "name": "chargemap/insider-sdk",
  "description": "Insider API SDK",
  "type": "library",
  "license": "BSD-3-Clause",
  "require": {
    "php": ">=7.4",
    "ext-mbstring": "*",
    "ext-json": "*",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "php-http/discovery": "^1.14",
    "myclabs/php-enum": "^1.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "slim/psr7": "1.3.0",
    "guzzlehttp/guzzle": "^7.4"
  },
  "suggest": {
    "slim/psr7": "1.3.0",
    "guzzlehttp/guzzle": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "Chargemap\\InsiderSdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Chargemap\\InsiderSdk\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
